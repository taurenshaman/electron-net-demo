<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="target-densitydpi=device-dpi, width=device-width, initial-scale=1.0, maximum-scale=1" />
    <meta name="charset" content="utf-8" />
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    @*<link rel="shortcut icon" type="image/x-icon" href="assets/images/favicon.ico" />
        <link rel="icon" type="image/png" href="assets/images/logo-256x256.png" sizes="256x256">
        <link rel="icon" type="image/png" href="assets/images/logo-128x128.png" sizes="128x128">
        <link rel="icon" type="image/png" href="assets/images/logo-64x64.png" sizes="64x64">
        <link rel="apple-touch-icon" sizes="256x256" href="assets/images/logo-256x256.png">
        <link rel="apple-touch-icon" sizes="128x128" href="assets/images/logo-128x128.png">
        <link rel="apple-touch-icon" sizes="64x64" href="assets/images/logo-64x64.png">*@
    <meta name="robots" content="noindex, nofollow" />
    @RenderSection( "HeadMeta", required: false )
    <title>@ViewBag.Title - Electron.Net</title>

    @RenderSection( "HeadLink", required: false )
    <link rel="stylesheet" href="~/lib/metro/css/metro-all.min.css?v=4.2.46" />
    <link rel="stylesheet" href="~/css/site.css" />
    @RenderSection( "CssPart", required: false )
    <script type="text/javascript" src="~/js/jquery/jquery-3.4.1.min.js"></script>
</head>

<body>
    @*@Html.PartialAsync( "_HeaderNavigation" )*@
    <partial name="_HeaderNavigation" />

    <div id="body" class="main-content">
        @RenderSection( "featured", required: false )

        @RenderBody()
    </div>

    @*@Html.PartialAsync( "_Footer" )*@
    <partial name="_Footer" />

    <environment include="Development">
        <script type="text/javascript" src="~/js/jquery/jquery-3.4.1.js"></script>
        <script type="text/javascript" src="~/lib/metro/js/metro.js?v=4.2.46"></script>
    </environment>
    <environment exclude="Development">
        <script type="text/javascript" src="~/js/jquery/jquery-3.4.1.min.js"></script>
        @*<script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js"
                asp-fallback-src="~/js/jquery/jquery-3.4.1.min.js"
                asp-fallback-test="window.jQuery"
                crossorigin="anonymous"
                integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=">
        </script>*@
        @*<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.bundle.min.js"
                asp-fallback-src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"
                asp-fallback-test="window.jQuery && window.jQuery.fn && window.jQuery.fn.modal"
                crossorigin="anonymous"
                integrity="sha384-xrRywqdh3PHs8keKZN+8zzc5TX0GRTLCcmivcbNJWm2rs5C8PRhcEn3czEjhAO9o">
        </script>*@
        <script type="text/javascript" src="~/lib/metro/js/metro.min.js?v=4.2.46"></script>
    </environment>

    <script type="text/javascript" src="~/js/common.js" asp-append-version="true"></script>
    @RenderSection( "JsPart", required: false )
    @RenderSection( "ScriptPart", required: false )
</body>

</html>
